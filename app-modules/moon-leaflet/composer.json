{
	"name": "muxoz/moon-leaflet",
	"description": "Leaflet field for MoonShine",
	"keywords": [
		"moonshine",
		"leaflet",
		"map"
	],
	"type": "library",
	"version": "1.0",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/modules/moon-leaflet/issues",
		"source": "https://github.com/modules/moon-leaflet"
	},
	"authors": [
		{
			"name": "Maycol Mu√±oz",
			"email": "macarny4@gmail.com"
		}
	],
	"require": {
		"php": "^8.2|^8.3"
	},
	"require-dev": {
		"moonshine/moonshine": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"Muxoz\\MoonLeaflet\\": "src/",
			"Muxoz\\MoonLeaflet\\Tests\\": "tests/",
			"Muxoz\\MoonLeaflet\\Database\\Factories\\": "database/factories/",
			"Muxoz\\MoonLeaflet\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Muxoz\\MoonLeaflet\\Providers\\MoonLeafletServiceProvider"
			]
		}
	}
}